% Implementation of the sliding window function
%function SW = SlidingWindow(y)

counter = 0;

for k=1:humanrate
    w_y(k)=A1(k);
    if mod(k,w) == 0
        %do processing function
        %SW(k+1-w:k) = 2*w_y(k+1-w:k);
        SW(k) = mean(w_y(k+1-w:k));
        counter = counter+1;
    end
end

%end